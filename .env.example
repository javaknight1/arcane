# Arcane Configuration
# Copy this file to .env and fill in your values

# Required
ARCANE_ANTHROPIC_API_KEY=your-key-here
ARCANE_MODEL=claude-sonnet-4-20250514
ARCANE_MAX_RETRIES=3

# Optional - Project Management
ARCANE_LINEAR_API_KEY=
ARCANE_JIRA_DOMAIN=
ARCANE_JIRA_EMAIL=
ARCANE_JIRA_API_TOKEN=
ARCANE_NOTION_API_KEY=

# Behavior
ARCANE_INTERACTIVE=true
ARCANE_AUTO_SAVE=true
ARCANE_OUTPUT_DIR=./
